{
  "name": "snakes",
  "displayName": "Snakes",
  "description": "Language support for CS4410",
   "publisher": "Emery",
  "repository": "https://github.com/ThePineappleW/snakes",
  "version": "0.0.3",
  "engines": {
    "vscode": "^1.21.0"
  },
  "contributes": {
    "languages": [
      {
        "id": "adder",
        "aliases": ["Adder", "adder"],
        "extensions": [".adder"],
        "configuration": "./configs/adder-language-configuration.json"
      },
      {
        "id": "boa",
        "aliases": ["Boa", "boa"],
        "extensions": [".boa"],
        "configuration": "./configs/boa-language-configuration.json"
      },
      {
        "id": "cobra",
        "aliases": ["Cobra", "cobra"],
        "extensions": [".cobra"],
        "configuration": "./configs/cobra-language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "adder",
        "scopeName": "source.adder",
        "path": "./syntaxes/adder.tmLanguage.json"
      },
      {
        "language": "boa",
        "scopeName": "source.boa",
        "path": "./syntaxes/boa.tmLanguage.json"
      },
      {
        "language": "cobra",
        "scopeName": "source.cobra",
        "path": "./syntaxes/cobra.tmLanguage.json"
      }
    ]
  }
}